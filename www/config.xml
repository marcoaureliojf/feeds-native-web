<?xml version="1.0" encoding="utf-8"?>
<widget id="com.phonegap.sensation.feed" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="EmailComposer">
        <param name="android-package" value="de.appplant.cordova.plugin.emailcomposer.EmailComposer" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>SensationFeed</name>
    <description> Sensation Feed Reader is a PhoneGap/Cordova Full Hybrid App with AngularJS + Onsen UI. Sensation Feed Reader is already part of the Sensation App.
</description>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
    <content src="index.html" />
    <access origin="*" />
</widget>